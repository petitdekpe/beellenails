{% block title %}Creneau{% endblock %}

{% block body %}
    <h1>Creneau</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ creneau.id }}</td>
            </tr>
            <tr>
                <th>StartTime</th>
                <td>{{ creneau.startTime ? creneau.startTime|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndTime</th>
                <td>{{ creneau.endTime ? creneau.endTime|date('H:i:s') : '' }}</td>
